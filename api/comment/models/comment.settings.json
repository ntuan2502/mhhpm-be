{
  "kind": "collectionType",
  "collectionName": "comments",
  "info": {
    "name": "Comment"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "stars": {
      "type": "integer",
      "max": 5,
      "min": 1
    },
    "value": {
      "type": "richtext"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "comments"
    },
    "food": {
      "via": "comments",
      "model": "food"
    }
  }
}
