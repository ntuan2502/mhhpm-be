{
  "kind": "collectionType",
  "collectionName": "bill_details",
  "info": {
    "name": "billDetail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "food": {
      "model": "food",
      "via": "bill_details"
    },
    "quantity": {
      "type": "integer"
    },
    "bill": {
      "via": "bill_details",
      "model": "bill"
    },
    "prices": {
      "type": "float",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "null",
        "pending",
        "accept",
        "reject"
      ]
    },
    "description": {
      "type": "richtext"
    }
  }
}
